<div class="home flex flex-direction justifyContent-center alignItems-center">
  <%= form_for(:session, url: login_path) do |f| %>
    <p><%= f.label :email %>
    <%= f.email_field :email %></p>

    <p><%= f.label :password %>
    <%= f.password_field :password %></p>

    <%= f.submit "ログイン" %>
  <% end %>
</div>